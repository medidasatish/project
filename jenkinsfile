pipeline {
         agent any
                 stages {
                        stages('one') {
                                 steps  {
                                           echho 'hi satish '
                                   }
                          }
                          stage('two') {
                                  steps  {
                                           mvn --version
                                    }
                           }
                           stage('three') {
                                  when {
                                           not {
                                                  branch "master"
                                            }
                                      }
                                      steps {
                                              echho "hello"
                                       }
                              }
                              stage('four')  {
                                              parallel {
                                                 stage('Unit Test') {
                                                                       steps{
                                                                             echo " Running the unit test.."
                                                                        }
                                                 }
                                                 stage('Integration test'){
                                                                     agent{
                                                                           docker {
                                                                                    reuseNode false
                                                                                    image 'ubuntu'
                                                                             }
                                                   }
                                                   steps {
                                                          echo 'Running the interation test....'
                                                    }
                                             }
                                   }
              }
              }
              }
              }
                                             
                                                  
                                               
                                          
